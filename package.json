{
  "name": "robby-robot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run dev:worker\" \"ng serve\"",
    "build": "npm run build:worker && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:worker": "ng run robby-robot:build-worker",
    "dev:worker": "ng run robby-robot:serve-worker"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",

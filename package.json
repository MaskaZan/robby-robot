    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
